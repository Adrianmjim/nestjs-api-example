{
  "name": "nestjs-api-example",
  "version": "0.1.0",
  "description": "HTTP REST API pet project built on top of Nestjs",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:incremental": "tsc -p tsconfig.build.incremental.json",
    "build:git:hook": "tsc -p tsconfig.build.gitHook.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect=0.0.0.0:9229 dist/main.js\"",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "test:coverage": "npm test -- --coverage",
    "test:develop": "jest --changedSince=origin/develop",
    "test:previous": "jest --changedSince=HEAD~1",
    "test:js:ci": "npm run test:js -- --ci",
    "test:js:ci:coverage": "npm run test:js:ci -- --coverage",
    "test:js:coverage": "npm run test:js -- --coverage",
    "test:js": "npm test -- --config=jest.js.config.json",
    "test:unit": "jest --selectProjects Unit",
    "test:unit:js": "npm run test:js -- --selectProjects Unit",
    "prepare": "husky install"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/apollo": "^10.0.9",
    "@nestjs/axios": "0.0.3",
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/cqrs": "^8.0.1",
    "@nestjs/graphql": "^10.0.10",
    "@nestjs/platform-fastify": "^8.2.6",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.3",
    "apollo-server-fastify": "^3.6.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookies": "^0.8.0",
    "dataloader": "^2.1.0",
    "dotenv": "^14.2.0",
    "envalid": "^7.2.2",
    "eslint-plugin-import": "^2.25.4",
    "fastify": "3.27.0",
    "fastify-swagger": "^4.13.1",
    "graphql": "^16.4.0",
    "graphql-scalars": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cookies": "^0.7.7",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.12",
    "@types/passport-jwt": "^3.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "enhanced-resolve": "^5.8.3",
    "eslint": "^8.7.0",
    "eslint-plugin-jest": "^25.7.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.2.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.6.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "~4.5.4"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  }
}
