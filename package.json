{
  "name": "nestjs-api-example",
  "version": "0.1.0",
  "description": "HTTP REST API pet project built on top of Nestjs",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:incremental": "tsc -p tsconfig.build.incremental.json",
    "build:git:hook": "tsc -p tsconfig.build.gitHook.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect=0.0.0.0:9229 dist/main.js\"",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "test:coverage": "npm test -- --coverage",
    "test:develop": "jest --changedSince=origin/develop",
    "test:previous": "jest --changedSince=HEAD~1",
    "test:js:ci": "npm run test:js -- --ci",
    "test:js:ci:coverage": "npm run test:js:ci -- --coverage",
    "test:js:coverage": "npm run test:js -- --coverage",
    "test:js": "npm test -- --config=jest.js.config.json",
    "test:unit": "jest --selectProjects Unit",
    "test:unit:js": "npm run test:js -- --selectProjects Unit",
    "prepare": "husky install"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mikro-orm/core": "5.9.0",
    "@mikro-orm/migrations": "5.9.0",
    "@mikro-orm/nestjs": "5.2.2",
    "@mikro-orm/postgresql": "5.9.0",
    "@nestjs/apollo": "10.2.1",
    "@nestjs/axios": "0.1.1",
    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "@nestjs/cqrs": "9.0.4",
    "@nestjs/graphql": "10.2.1",
    "@nestjs/platform-fastify": "9.4.3",
    "@nestjs/swagger": "6.3.0",
    "apollo-server-fastify": "3.12.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookies": "0.8.0",
    "dataloader": "2.2.2",
    "dotenv": "16.3.1",
    "envalid": "7.3.1",
    "eslint-plugin-import": "2.29.0",
    "fastify": "4.24.3",
    "fastify-swagger": "5.2.0",
    "graphql": "16.8.1",
    "graphql-scalars": "1.22.4",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@types/cookies": "0.7.9",
    "@types/jest": "29.5.6",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "16.18.59",
    "@types/passport-jwt": "3.0.12",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "enhanced-resolve": "5.15.0",
    "eslint": "8.52.0",
    "eslint-plugin-jest": "27.6.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "13.3.0",
    "prettier": "2.8.8",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsc-watch": "5.0.3",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  }
}
