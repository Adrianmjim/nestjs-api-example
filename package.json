{
  "name": "nestjs-api-example",
  "version": "0.1.0",
  "description": "HTTP REST API pet project built on top of Nestjs",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:incremental": "tsc -p tsconfig.build.incremental.json",
    "build:git:hook": "tsc -p tsconfig.build.gitHook.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect=0.0.0.0:9229 dist/main.js\"",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "test:coverage": "npm test -- --coverage",
    "test:develop": "jest --changedSince=origin/develop",
    "test:previous": "jest --changedSince=HEAD~1",
    "test:js:ci": "npm run test:js -- --ci",
    "test:js:ci:coverage": "npm run test:js:ci -- --coverage",
    "test:js:coverage": "npm run test:js -- --coverage",
    "test:js": "npm test -- --config=jest.js.config.json",
    "test:unit": "jest --selectProjects Unit",
    "test:unit:js": "npm run test:js -- --selectProjects Unit",
    "prepare": "husky install"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mikro-orm/core": "5.4.2",
    "@mikro-orm/migrations": "5.4.2",
    "@mikro-orm/nestjs": "5.1.2",
    "@mikro-orm/postgresql": "5.4.2",
    "@nestjs/apollo": "12.0.9",
    "@nestjs/axios": "3.0.1",
    "@nestjs/common": "10.2.8",
    "@nestjs/core": "10.2.8",
    "@nestjs/cqrs": "10.2.6",
    "@nestjs/graphql": "12.0.9",
    "@nestjs/platform-fastify": "10.2.8",
    "@nestjs/swagger": "7.1.14",
    "apollo-server-fastify": "3.10.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookies": "0.8.0",
    "dataloader": "2.1.0",
    "dotenv": "16.0.2",
    "envalid": "8.0.0",
    "eslint-plugin-import": "2.26.0",
    "fastify": "4.6.0",
    "fastify-swagger": "5.2.0",
    "graphql": "16.6.0",
    "graphql-scalars": "1.18.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.5.6"
  },
  "devDependencies": {
    "@types/cookies": "0.7.7",
    "@types/jest": "29.0.2",
    "@types/jsonwebtoken": "9.0.4",
    "@types/node": "20.8.10",
    "@types/passport-jwt": "3.0.6",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "enhanced-resolve": "5.10.0",
    "eslint": "8.23.1",
    "eslint-plugin-jest": "27.0.4",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "ts-jest": "29.0.1",
    "ts-node": "10.9.1",
    "tsc-watch": "6.0.4",
    "tsconfig-paths": "4.1.0",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  }
}
