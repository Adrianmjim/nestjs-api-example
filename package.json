{
  "name": "nestjs-api-example",
  "version": "0.1.0",
  "description": "HTTP REST API pet project built on top of Nestjs",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:incremental": "tsc -p tsconfig.build.incremental.json",
    "build:git:hook": "tsc -p tsconfig.build.gitHook.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect=0.0.0.0:9229 dist/main.js\"",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "test:coverage": "npm test -- --coverage",
    "test:develop": "jest --changedSince=origin/develop",
    "test:previous": "jest --changedSince=HEAD~1",
    "test:js:ci": "npm run test:js -- --ci",
    "test:js:ci:coverage": "npm run test:js:ci -- --coverage",
    "test:js:coverage": "npm run test:js -- --coverage",
    "test:js": "npm test -- --config=jest.js.config.json",
    "test:unit": "jest --selectProjects Unit",
    "test:unit:js": "npm run test:js -- --selectProjects Unit",
    "prepare": "husky install"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mikro-orm/core": "5.3.1",
    "@mikro-orm/migrations": "5.3.1",
    "@mikro-orm/nestjs": "5.1.1",
    "@mikro-orm/postgresql": "5.3.1",
    "@nestjs/apollo": "10.0.22",
    "@nestjs/axios": "0.0.3",
    "@nestjs/common": "8.4.7",
    "@nestjs/core": "8.4.7",
    "@nestjs/cqrs": "8.0.5",
    "@nestjs/graphql": "10.0.22",
    "@nestjs/platform-fastify": "8.4.7",
    "@nestjs/swagger": "5.2.1",
    "apollo-server-fastify": "3.10.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dataloader": "^2.1.0",
    "dotenv": "14.3.2",
    "envalid": "7.3.1",
    "eslint-plugin-import": "2.26.0",
    "fastify": "3.27.0",
    "fastify-swagger": "4.17.1",
    "graphql": "16.6.0",
    "graphql-scalars": "1.18.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6"
  },
  "devDependencies": {
    "@types/cookies": "0.7.7",
    "@types/jest": "27.5.2",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "16.11.56",
    "@types/passport-jwt": "3.0.6",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.34.0",
    "@typescript-eslint/parser": "5.34.0",
    "enhanced-resolve": "5.10.0",
    "eslint": "8.22.0",
    "eslint-plugin-jest": "25.7.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.5.0",
    "prettier": "2.7.1",
    "ts-jest": "27.1.5",
    "ts-node": "10.9.1",
    "tsc-watch": "4.6.2",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  }
}
